{
  "expo": {
    "name": "eats 92",
    "slug": "food-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pavan1984.foodapp",
      "icon": "./assets/playstore.png"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "icon": "./assets/adaptive-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.pavan1984.foodapp",
      "permissions": ["NOTIFICATIONS","LOCATION"]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/playstore.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/adaptive-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/notification.wav"
          ],
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3fe416bc-961c-4279-9b1d-5bada22e9d53"
      }
    }
  }
}
