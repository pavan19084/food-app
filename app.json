{
  "expo": {
    "name": "92 eats",
    "slug": "food-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/bg-remove-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B35"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pavan1984.foodapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./assets/playstore.png"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "resize",
      "icon": "./assets/playstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#FF6B35"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pavan1984.foodapp"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/playstore.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "extra": {
      "eas": {
        "projectId": "3fe416bc-961c-4279-9b1d-5bada22e9d53"
      }
    }
  }
}
